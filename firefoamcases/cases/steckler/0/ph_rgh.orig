FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      ph_rgh.orig;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 0;
    }
    sides
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    base
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    burner
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    floor
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    baffle1DWall_master
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
    baffle1DWall_slave
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }
}


