FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      qr;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    panel_top
    {
        type            zeroGradient;
    }

    region0_to_panelRegion_panel
    {
        type            mappedField;
        sampleRegion    region0;
        sampleMode      nearestPatchFace;
        samplePatch     region0_to_panelRegion_panel;
        offset          (0 0 0);
        fieldName       qr;
        setAverage      no;
        average         0;
        value           uniform 0;
    }
}


