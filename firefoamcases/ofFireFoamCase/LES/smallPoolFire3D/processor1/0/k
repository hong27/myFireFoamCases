FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.0001;

boundaryField
{
    base
    {
        type            zeroGradient;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.0001;
        value           uniform 0.0001;
    }
    sides
    {
        type            inletOutlet;
        inletValue      uniform 0.0001;
        value           uniform 0.0001;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.0001;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 0.0001;
    }
}


