FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    base
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    outlet
    {
        type            calculated;
        value           uniform 101325;
    }
    sides
    {
        type            calculated;
        value           uniform 101325;
    }
    inlet
    {
        type            calculated;
        value           nonuniform 
0
;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 101325;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 101325;
    }
}


