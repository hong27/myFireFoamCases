FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    base
    {
        type            zeroGradient;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    sides
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 300;
    }
}


